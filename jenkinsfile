pipeline {
       agent {
               label 'agentnode1'
       }
       stages {
               stage ('pull scm') {
                       steps {
                               checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'githubganesh', url: 'https://github.com/ganesh5556/tokengenerator.git']])
                       }
               }
               stage('build') {
                      steps {
                              sh "mvn clean verify"
                      }
               }
        }
}
